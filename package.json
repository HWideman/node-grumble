{
  "name": "node-grumble",
  "version": "0.0.0-development",
  "main": "build/index.js",
  "author": "Armando MagalhÃ£es",
  "license": "MIT",
  "files": [
    "build"
  ],
  "keywords": [
    "mumble",
    "grumble",
    "client",
    "typescript",
    "voice"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": "armand1m/node-grumble",
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/zen-observable": "^0.8.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "ts-proto": "^1.39.0",
    "ts-protoc-gen": "^0.13.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "fluent-ffmpeg": "^2.1.2",
    "long": "^4.0.0",
    "protobufjs": "^6.10.2",
    "tslib": "^2.0.3",
    "zen-observable": "^0.8.15"
  },
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "tsc",
    "postbuild": "cp ./src/proto/Mumble.proto ./build/proto",
    "lint": "prettier --check './src/**/*.{tsx,ts}'",
    "lint:fix": "prettier --write './src/**/*.{tsx,ts}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "generate:proto": "protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./src/generated ./src/proto/Mumble.proto",
    "start:example": "ts-node ./src/example",
    "pack:inspect": "yarn pack && tar -ztvf *.tgz",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
